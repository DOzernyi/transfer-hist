@incollection{avermaet_language_2013,
	title = {Language {Testing} for {Immigration} to {Europe}},
	volume = {Volume I. Abilities, Contexts, and Learners. Part 3. Assessment Contexts},
	isbn = {978-1-118-41136-0},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/9781118411360.wbcla132},
	abstract = {Since the beginning of the 21st century, integration and citizenship discourses and policies have become more and more intertwined in contemporary superdiverse societies in Western Europe. Flanders (Belgium) is a clear example of how integration is gradually being replaced by virtual or moral citizenship. The concept of citizenship is used in multiple ways: citizenship as nationality, but also citizenship as a set of norms and values (which immigrants particularly have to comply with) and citizenship as identification with the host society. The authors argue that the “integration policy” in Flanders is in fact a hidden “citizenship policy” that includes the old, obsolete, and static concepts and definitions of monolingualism, nationality, and ethnicity. In these renewed integration policies, proficiency in the dominant language and knowledge of the host society are seen as conditions for social participation, therefore new immigrants have to take a compulsory integration course and “older” migrants are made to enroll in Dutch language courses. Increasingly, standardized tests—and especially language tests—are used to determine the learning outcomes of these courses and to regulate possibilities of social and economic participation. The authors advocate looking at citizenship and the use of plurilingual repertoires in the city as dynamic and contextualized processes, shaped in daily practice through negotiation in social networks. In a context where “superdiversity” is becoming the norm, it is important to reflect on the boundaries of the current recipes—integration policies including obligatory language tests—that are being used to promote and strengthen social and civic activity.},
	language = {en},
	urldate = {2021-08-27},
	booktitle = {The {Companion} to {Language} {Assessment}},
	publisher = {American Cancer Society},
	author = {Avermaet, Piet Van and Pulinx, Reinhilde},
	editor = {Kunnan, A.J.},
	year = {2013},
	keywords = {Citizenship, Emigration, Immigration, Western Europe},
	pages = {376--389},
}

