@article{bot_what_2015,
	title = {What is special about {L3} processing?*},
	volume = {18},
	issn = {1366-7289, 1469-1841},
	shorttitle = {What is special about {L3} processing?},
	url = {https://www.cambridge.org/core/journals/bilingualism-language-and-cognition/article/what-is-special-about-l3-processing/1276AB3752437C103517C0D6B417E1AB},
	doi = {10.1017/S1366728913000448},
	abstract = {While research on third language (L3) and multilingualism has recently shown remarkable growth, the fundamental question of what makes trilingualism special compared to bilingualism, and indeed monolingualism, continues to be evaded. In this contribution we consider whether there is such a thing as a true monolingual, and if there is a difference between dialects, styles, registers and languages. While linguistic and psycholinguistic studies suggest differences in the processing of a third, compared to the first or second language, neurolinguistic research has shown that generally the same areas of the brain are activated during language use in proficient multilinguals. It is concluded that while from traditional linguistic and psycholinguistic perspectives there are grounds to differentiate monolingual, bilingual and multilingual processing, a more dynamic perspective on language processing in which development over time is the core issue, leads to a questioning of the notion of languages as separate entities in the brain.},
	language = {en},
	number = {2},
	urldate = {2021-10-17},
	journal = {Bilingualism: Language and Cognition},
	author = {Bot, Kees De and Jaensch, Carol},
	month = apr,
