@article{reed_repetition_1917,
	title = {A repetition of {Ebert} and {Meumann}'s practice experiment on memory},
	volume = {2},
	issn = {0022-1015(Print)},
	doi = {10.1037/h0073769},
	abstract = {Investigated the effects of practice upon memory by repeating Ebert and Meumann's practice experiment on memory. Two series of tests were given: 7 memory-span tests, and 6 learning tests. Memory-span tests were presented in an auditory form, and the learning tests were given visually and were relearned after 24 hrs. 13 Ss were divided into: Group I with 8 Ss, and Group II with 5 Ss. Group II was the control group and performed only the test series, whereas Group I performed the test as well as the practice series. The relearning curves obtained either varied about a horizontal line or else ran closely parallel to the learning curves. Cooperation of will did not appear to be absolute and of controlling importance, as stated by Muemann, for no exertion of the will prevented bad records or plateaus. With regard to transfer of training, Meumann's results and conclusions have not been confirmed. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
