@article{kim_sequential_2022,
	title = {Sequential bilingual heritage children's {L1} attrition in lexical retrieval: {Age} of acquisition versus language experience},
	issn = {1366-7289, 1469-1841},
	shorttitle = {Sequential bilingual heritage children's {L1} attrition in lexical retrieval},
	url = {https://www.cambridge.org/core/journals/bilingualism-language-and-cognition/article/sequential-bilingual-heritage-childrens-l1-attrition-in-lexical-retrieval-age-of-acquisition-versus-language-experience/B41BE0ECBD3B40678FFE22337B6C37C1},
	doi = {10.1017/S1366728921001139},
	abstract = {This study investigated the unresolved issue of potential sources of heritage language attrition. To test contributing effects of three learner variables – age of second language acquisition, length of residence, and language input – on heritage children's lexical retrieval accuracy and speed, we conducted a real-time word naming task with 68 children (age 11–14 years) living in South Korea who spoke either Chinese or Russian as a heritage language. Results of regression analyses showed that the participants were less accurate and slower in naming target words in their heritage language as their length of residence in Korea and the amount of Korean input increased. The age of Korean acquisition did not significantly influence their performance. These findings support the claim that heritage speakers’ language experience is a more reliable predictor of first language attrition than age of acquisition. We discuss these findings in light of different approaches to explaining language attrition.},
	language = {en},
	urldate = {2022-01-14},
	journal = {Bilingualism: Language and Cognition},
	author = {Kim, Kitaek and Kim, Hyunwoo},
	month = jan,
	year = {2022},
	note = {Publisher: Cambridge University Press},
	keywords = {age of acquisition, heritage speakers, language attrition, language experience, lexical retrieval},
	pages = {1--11},
}

@article{cheng_double-number_2021,
	title = {{DOUBLE}-{NUMBER} {MARKING} {MATTERS} {FOR} {BOTH} {L1} {AND} {L2} {PROCESSING} {OF} {NONLOCAL} {AGREEMENT} {SIMILARLY}: {AN} {ERP} {INVESTIGATION}},
	issn = {0272-2631, 1470-1545},
	shorttitle = {{DOUBLE}-{NUMBER} {MARKING} {MATTERS} {FOR} {BOTH} {L1} {AND} {L2} {PROCESSING} {OF} {NONLOCAL} {AGREEMENT} {SIMILARLY}},
	url = {https://www.cambridge.org/core/journals/studies-in-second-language-acquisition/article/doublenumber-marking-matters-for-both-l1-and-l2-processing-of-nonlocal-agreement-similarly/2748856FBE76E8FAF8DED91A1FC5365D},
	doi = {10.1017/S0272263121000772},
	abstract = {The present study uses event-related potentials (ERPs) to examine nonlocal agreement processing between native (L1) English speakers and Chinese–English second language (L2) learners, whose L1 lacks number agreement. We manipulated number marking with determiners (the vs. that/these) to see how determiner-specification influences both native and nonnative processing downstream for verbal number agreement. Behavioral and ERP results suggest both groups detected nonlocal agreement violations, indexed by a P600 effect. Moreover, the manipulation of determiner-number specification revealed a facilitation effect across the board in both grammaticality judgment and ERP responses for both groups: increased judgment accuracy and a larger P600 effect amplitude for sentences containing violations with demonstratives rather than bare determiners. Contrary to some claims regarding the potential for nonnative processing, the present data suggest that L1 and L2 speakers show similar ERP responses when processing agreement, even when the L1 lacks the relevant distinction.},
	language = {en},
	urldate = {2022-01-13},
	journal = {Studies in Second Language Acquisition},
	author = {Cheng, Yesi and Cunnings, Ian and Miller, David and Rothman, Jason},
	month = dec,
	year = {2021},
	note = {Publisher: Cambridge University Press},
	pages = {1--21},
}

